{
  "name": "next-route-progress",
  "version": "1.0.3",
  "description": "A lightweight progress bar for Next.js App Router (a modern alternative to nprogress).",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nextjs",
    "progress",
    "loading-bar",
    "app-router",
    "react",
    "typescript",
    "nprogress"
  ],
  "author": "Isaac Antwi",
  "homepage": "https://www.okraks.fyi/next-route-progress",
  "repository": {
    "type": "git",
    "url": "https://github.com/okraks/next-route-progress"
  },
  "bugs": {
    "url": "https://github.com/okraks/next-route-progress/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "next": ">=13.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.4.0"
  }
}
